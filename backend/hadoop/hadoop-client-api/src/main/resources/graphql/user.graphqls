type User {
    userId: ID!
    email: String!
    name: String!
    fundInfo: FundInfo!
    ledgerInfo: LedgerInfo!
    appInfo: AppInfo!
}

type FundInfo {
    fundIds: [ID]!
    delFundIds: [ID]!
}

type LedgerInfo {
    ledgerIds: [ID]!
    delLedgerIds: [ID]!
}

type AppInfo {
    categories: [Category]!
    subCategories: [Subcategory]!
}

type Query {
    getUser: User
}